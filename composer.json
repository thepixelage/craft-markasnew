{
    "name": "thepixelage/craft-fresh",
    "description": "Fresh is a Craft CMS plugin for marking content as new",
    "type": "craft-plugin",
    "version": "4.0.0-beta.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft-fresh"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "ThePixelAge",
            "homepage": "https://www.thepixelage.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "craftcms/cms": "^4.0.0-beta",
        "ext-json": "*"
