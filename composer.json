    "autoload": {
        "psr-4": {
          "thepixelage\\fresh\\": "src/"
        }
    },
    "extra": {
        "name": "Fresh",
        "handle": "fresh",
        "developer": "ThePixelAge",
        "developerUrl": "https://www.thepixelage.com",
